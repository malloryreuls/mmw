<div class="container">   

  <div class="row">
    <div class="large-3 columns">
      <div style="margin-top: 15%">
        <%= image_tag "logo_sm.png" %>
      </div>
    </div>
  </div>

  <div class="row">    
    <div class="large-9 push-3 columns" style="margin-top: -5%">
    <h3>Most Frequent Search Queries</h3>

    <table>
      <thead>
        <tr>
          <th width="200">Query Name</th>
          <th>Count:</th>
        </tr>
      </thead>
      <tbody>
        <% @history_count.each do |key, value| %>
        <tr>
          <td><%= key %></td>
          <td><%= value %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
    </div>


    <div class="large-3 pull-9 columns">

      <ul class="side-nav">
         <li><%= link_to "Most Recent Searches", admins_path %></li>
        <li><%= link_to "Most Frequent Searches", dashboard_index_path %></li>
      </ul>
    </div>
  </div>

  

  
  <footer class="row">
    <div class="large-12 columns">
      <hr/>
      <div class="row">
        <div class="large-6 columns">
          <p>Â© Copyright: Ask DMX or BUST</p>
        </div>
        <div class="large-6 columns">
          <ul class="inline-list right">
            <li><a href="https://github.com/malloryreuls/mmw">View GitHub Repo</a></li>
          </ul>
        </div>
      </div>
    </div> 
  </footer>
  
</div>

  <div class="main">

    <div id="searches-destination"></div>

  </div>

  <script id="searches-template" type="text/x-handlebars-template">
  {{#each searches}}

  <p>Query: {{query}} Created at: {{created_at}}</p>


  {{/each}}
  </script>


  <script>$(function() { loadSearches(); }); </script>