  <div class="container">
  <h1><%= @query.name%></h1>

  <%= render partial: 'form' %>

  <div class="row">

    <div class="large-6 columns panel" style="text-align: left">
      <ul class=class="small-block-grid-3">
        <li><%= @query.description %></li>
      </ul>
    </div>

    <div class="large-6 columns">
      <ul class="small-block-grid-3">
     
        <% @googleimage.each do |image| %>
      
       	<li class="th"><img src='<%=image["unescapedUrl"]%>'></li> 
        <% end %>
        
      </ul>
    </div>
  </div>

<center><hr width="80%"></center>

  <div class="row">
    <div class="medium-11 medium-centered columns">
     <ul class="small-block-grid-3">
      <% for youtube in @youtube %>

      <li><iframe width="250px" src='<%= "http://www.youtube.com/embed/#{youtube}" %>' frameborder="0">
      </iframe></li>
      <% end %>
    </ul>
  </div>
</div>
</div>

